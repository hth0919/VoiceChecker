<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefHeight="330.0" prefWidth="411.0" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="recbutton" layoutX="102.0" layoutY="111.0" mnemonicParsing="false" onAction="#handlerec" prefHeight="71.0" prefWidth="82.0">
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="56.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@resource/rec.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="stopbutton" layoutX="214.0" layoutY="110.0" mnemonicParsing="false" onAction="#handlestop" prefHeight="71.0" prefWidth="82.0">
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="56.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@resource/stop.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Label fx:id="idshowlabel" layoutX="177.0" layoutY="7.0" prefHeight="18.0" prefWidth="90.0" text="" />
      <Label fx:id="welcomlabel" layoutX="259.0" layoutY="7.0" prefHeight="18.0" prefWidth="152.0" text="" />
      <Button fx:id="loginbutton" layoutX="103.0" layoutY="212.0" mnemonicParsing="false" onAction="#handlelogin" prefHeight="32.0" prefWidth="82.0" text="Login" />
      <Button fx:id="regbutton" layoutX="213.0" layoutY="212.0" mnemonicParsing="false" onAction="#handleregister" prefHeight="32.0" prefWidth="82.0" text="Register" />
      <Label fx:id="statelabel" layoutX="130.0" layoutY="67.0" text="Stop" />
      <Label fx:id="timelabel" layoutX="232.0" layoutY="67.0" text="0:00" />
      
   </children>
</fx:root>
